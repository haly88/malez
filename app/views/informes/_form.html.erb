<table class="informe">
  <thead>
    <tr>
    	<% @resultado.columns.each do |c| %>
      	<th><%= c %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @resultado.rows.each do |c| %>
      <tr>
      <% c.each do |c| %>
        <td><%= c %></td>
      <% end %>
      </tr>
    <% end %>
  </tbody>
</table>